{
  "name": "bahaiwritings",
  "version": "0.44.0",
  "description": "The Baha'i Writings in JSON format",
  "type": "module",
  "scripts": {
    "tippy-rollup": "rollup -c",
    "eslint": "eslint --ext js,md,html .",
    "lint": "npm run eslint",
    "delay": "delay 5",
    "open-test-only": "open-cli \"http://localhost:8000/textbrowser?lang=en-US&work=aqdas&aqdas-start1-1=3&aqdas-end1-1=5&anchorfield1=Paragraph&field1=Paragraph&field2=English&field3=German&field4=Arabic&colorName=Black&color=%23&bgcolorName=White&bgcolor=%23&fontSeq=Times+New+Roman%2C+serif&fontstyle=normal&fontvariant=Normal&fontweight=normal&fontstretch=normal&letterspacing=normal&lineheight=normal&header=n&footer=0&caption=0&border=1&interlintitle=1&rand=No&=No&checked1=Yes&checked2=Yes&checked3=Yes&checked4=Yes&headerfooterfixed=No&result=Yes&outputmode=table&serverOutput=json\"",
    "open-test-only-delayed": "run-s delay open-test-only",
    "open-test": "run-p server open-test-only-delayed",
    "node": "npm run eslint && mocha --require chai/register-assert.js test/bahaiwritingsTests.js",
    "testcafe": "testcafe --skip-js-errors chrome test/ui-tests/**/*.js",
    "test": "npm run eslint && run-p server testcafe",
    "start": "static -p 8082",
    "open-only": "open-cli http://127.0.0.1:8082/test/",
    "open-only-delayed": "run-s delay open-only",
    "open": "run-p start open-only-delayed",
    "browser-test": "run-s eslint open"
  },
  "engines": {
    "node": ">=10.11.0"
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:brettz9/bahaiwritings.git"
  },
  "author": "Brett Zamir",
  "contributors": [],
  "keywords": [
    "bahai",
    "textbrowser"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://bitbucket.org/brettz9/bahaiwritings/issues?status=new&status=open"
  },
  "homepage": "https://bitbucket.org/brettz9/bahaiwritings",
  "dependencies": {},
  "devDependencies": {
    "@brettz9/eslint-plugin": "^1.0.4",
    "ajv": "8.11.2",
    "chai": "^4.3.7",
    "delay-cli": "^2.0.0",
    "eslint": "^8.27.0",
    "eslint-config-ash-nazg": "^34.4.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-array-func": "^3.1.7",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.6.2",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-no-unsanitized": "^4.0.1",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-sonarjs": "^0.16.0",
    "eslint-plugin-standard": "^4.1.0",
    "eslint-plugin-testcafe": "^0.2.1",
    "eslint-plugin-unicorn": "^44.0.2",
    "fast-json-patch": "3.1.1",
    "json-metaschema": "1.3.0",
    "json-refs": "^3.0.15",
    "mocha": "^10.1.0",
    "npm-run-all": "^4.1.5",
    "textbrowser-data-schemas": "^0.1.0"
  }
}
